{
    "name": "cpts-javascript-utilities",
    "version": "1.0.1",
    "description": "Common helper functions in javascript/typescript",
    "repository": "https://github.com/cptSwing/cpts-javascript-utilities.git",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
    },
    "author": "Jens Brandenburg <jens@jbrandenburg.de>",
    "license": "MIT",
    "scripts": {
        "build": "rm -rf dist && tsup index.ts --format esm,cjs --dts",
        "postbuild": "node ./_building/addJSDocLinksToD.ts"
    },
    "devDependencies": {
        "@types/react": "^19.2.6",
        "csstype": "^3.1.3"
    },
    "dependencies": {
        "tsup": "^8.5.1",
        "typescript": "^5.9.3"
    }
}
